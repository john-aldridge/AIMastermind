# AI Mastermind Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Configuration (optional - users can provide their own)
# VITE_OPENAI_API_KEY=sk-...

# Backend API URL (for token purchases and premium features)
# VITE_API_BASE_URL=https://api.aimastermind.com

# Firebase Configuration (for cloud sync - premium users)
# VITE_FIREBASE_API_KEY=
# VITE_FIREBASE_AUTH_DOMAIN=
# VITE_FIREBASE_PROJECT_ID=
# VITE_FIREBASE_STORAGE_BUCKET=
# VITE_FIREBASE_MESSAGING_SENDER_ID=
# VITE_FIREBASE_APP_ID=

# Supabase Configuration (alternative to Firebase)
# VITE_SUPABASE_URL=
# VITE_SUPABASE_ANON_KEY=

# Stripe Configuration (for payments)
# VITE_STRIPE_PUBLIC_KEY=

# Feature Flags
# VITE_ENABLE_CLOUD_SYNC=false
# VITE_ENABLE_PREMIUM_FEATURES=false
